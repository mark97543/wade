  "private": true,
  "type": "module",
  "workspaces": [
    "1_client",
    "_components",
    "contexts",
    "2_travelBlog",
    "directus-backend"
  ],
  "scripts": {
    "dev:client": "npm run dev --workspace=1_client -- --force",
